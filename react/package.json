{
  "name": "@ion-phaser/react",
  "version": "1.2.0",
  "description": "React specific wrapper for @ion-phaser/core",
  "keywords": [
    "ionic",
    "phaser",
    "framework",
    "react",
    "webcomponent",
    "mobile",
    "app",
    "webapp",
    "web component",
    "web components",
    "progressive web app",
    "pwa"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/proyecto26/ion-phaser.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist && rm -rf dist-transpiled",
    "compile": "npm run tsc && rollup -c",
    "tsc": "tsc -p .",
    "test": "jest"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@ion-phaser/core": "file:../"
  },
  "devDependencies": {
    "react": "^16.9.0",
    "phaser": "^3.19.0",
    "@types/react": "^16.9.2",
    "typescript": "^3.6.2",
    "rollup": "^1.20.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "phaser": "^3.19.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist",
      "dist-transpiled"
    ]
  }
}
