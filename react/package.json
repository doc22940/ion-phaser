    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "jest": "^23.0.0",
    "jest-dom": "^3.0.2",
    "np": "^3.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
