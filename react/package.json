{
  "name": "@ion-phaser/react",
  "version": "0.1.2",
  "description": "React specific wrapper for ion-phaser",
  "keywords": [
    "ionic",
    "phaser",
    "framework",
    "react",
    "webcomponent",
    "app",
    "web component",
    "web components"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/proyecto26/ion-phaser.git"
  },
